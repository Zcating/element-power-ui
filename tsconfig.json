{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationDir": "./dist",
    "sourceMap": true,
    "baseUrl": "./",
    "rootDir": ".",
    "types": [
      "webpack-env",
      "jest"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "power-ui/*": [
        "./components/power/*"
      ],
      "power-ui": [
        "./components/power/index"
      ],
      "vue-cdk/*": [
        "./components/cdk/*"
      ],
      "vue-cdk": [
        "./components/cdk/index"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "/**/*.ts",
    "./**/*.tsx",
    "./**/*.vue",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}